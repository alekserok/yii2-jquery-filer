    "description": "Yii2 widget for creativedream/jquery.filer",
    "keywords": ["yii2", "filer", "file upload"],
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Kozmin",
            "email": "promcalc@gmail.com"
        }
